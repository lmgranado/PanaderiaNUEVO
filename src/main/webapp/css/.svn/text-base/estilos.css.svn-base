	BODY{
		margin: 0px 0px 0px 0px;
		font-family: Verdana;
		font-size: 11px;
		font-weight: normal;
		font-variant: normal;
		background-color: white;
	}
	
	.jsvalidation{
		color: blue;
		font-size: 11px;
	}
	
	#contenedor-pie {
		width: 100%;
		background: #CCC;
		position: relative;
		bottom: 0 !importante;
		bottom: -1px;
		height: 40px;
}

	*[readonly]{
		 color: black;
		 background-color: #CCCCCC;
	}
		
	*[disabled]{
		 color: black;
		 background-color: #CCCCCC;
	}

	IMG{
		border: 0px;
		margin: 0px;
		vertical-align: middle;
	}
	
	INPUT{
		font-size: 11px;
		padding: 2px 1px 2px 1px;
		margin: 0px;
		border: 1px solid #D9D9D9;
		background-color: #FBFBFB;
	}
	
	.textarea{
		font-size: 11px;
		padding: 2px 1px 2px 1px;
		margin: 0px;
		border: 1px solid #D9D9D9;
		background-color: #FBFBFB;
	}
	
	SELECT{
		font-size: 11px;
		padding: 2px 1px 2px 1px;
		margin: 0px;
		border: 1px solid #D9D9D9;
		background-color: #FBFBFB;
	}
	
	INPUT.check{
		border: 0px solid transparent;
		background-color: #FBFBFB;
	}
	
	A {
	  color: black;
	  text-decoration: none;
	  font-weight: bold;
	  cursor: pointer;
	}
	
	A:hover{
		color: #8C9470;
	}
	
	.fondo_pie{
